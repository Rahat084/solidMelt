FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    coolingWall
    {
        type            calculated;
        value           $internalField;
    }

    heatingWall
    {
        type            calculated;
        value           $internalField;
    }

    insulated
    {
        type            calculated;
        value           $internalField;
    }
}

// ************************************************************************* //
